    "concurrently": "^3.5.1",
    "jsdom": "^11.5.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "keywords": [
    "redux",
    "flux",
    "rx",
    "reactive",
    "simple",
    "model",
